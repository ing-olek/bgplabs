name: localas
prefix: "clab"

mgmt:
  network: netlab_mgmt
  ipv4-subnet: 192.168.121.0/24
  # Note: 'start' not validated
topology:
  nodes:
    rtr:
      mgmt-ipv4: 192.168.121.101
      kind: linux
      restart-policy: 'no'
      image: quay.io/frrouting/frr:10.0.1
      runtime: docker
      group: as65000
      binds:
      - clab_files/rtr/daemons:/etc/frr/daemons
      - clab_files/rtr/hosts:/etc/hosts
    x1:
      mgmt-ipv4: 192.168.121.110
      kind: linux
      restart-policy: 'no'
      image: quay.io/frrouting/frr:10.0.1
      runtime: docker
      group: external,as65100
      binds:
      - clab_files/x1/daemons:/etc/frr/daemons
      - clab_files/x1/hosts:/etc/hosts
    x2:
      mgmt-ipv4: 192.168.121.111
      kind: linux
      restart-policy: 'no'
      image: quay.io/frrouting/frr:10.0.1
      runtime: docker
      group: external,as65101
      binds:
      - clab_files/x2/daemons:/etc/frr/daemons
      - clab_files/x2/hosts:/etc/hosts


  links:
  - endpoints:
    - "rtr:eth1"
    - "x1:eth1"
  - endpoints:
    - "rtr:eth2"
    - "x2:eth1"
